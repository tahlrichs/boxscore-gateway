{
  "name": "boxscore-gateway",
  "version": "1.0.0",
  "description": "Data gateway for BoxScore app - normalizes sports data providers",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && cp -r src/data dist/data",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "test": "jest",
    "migrate": "tsx src/scripts/runMigration.ts",
    "ingest-players": "tsx src/scripts/ingestPlayers.ts",
    "ingest-players-simple": "tsx src/scripts/ingestPlayersSimple.ts",
    "import-all-players": "tsx src/scripts/importAllPlayers.ts",
    "scrape-colors": "tsx src/scripts/scrapeTeamColors.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.93.2",
    "@types/pg": "^8.16.0",
    "axios": "^1.7.9",
    "compression": "^1.7.5",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.0.0",
    "jose": "^6.1.3",
    "pg": "^8.17.1",
    "redis": "^4.7.0",
    "uuid": "^11.0.4",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.5",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "eslint": "^9.18.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
